' Archimate-PlantUML

' Version
'######################################
!function ArchimateVersion()
  !$archimateVersion  =  "2.0.0beta1"
  !return $archimateVersion
!endfunction

!procedure ArchimateVersionDetails()
  rectangle ArchimateVersionDetailsArea <<legendArea>> [
  | PlantUML | **%version()** |
  | Archimate-PlantUML | **ArchimateVersion()** |
  ]
!endprocedure

